<div id="sidebar" class="sidebar responsive ace-save-state" data-sidebar="true" data-sidebar-scroll="true" data-sidebar-hover="true">
    <script type="text/javascript">
					try{ace.settings.loadState('sidebar')}catch(e){}
    </script>
    <div class="sidebar-shortcuts" id="sidebar-shortcuts">
        <div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">
            <button class="btn btn-success">
                <i class="ace-icon fa fa-signal"></i>
            </button>
            <button class="btn btn-info">
                <i class="ace-icon fa fa-pencil"></i>
            </button>
            <button class="btn btn-warning">
                <i class="ace-icon fa fa-users"></i>
            </button>
            <button class="btn btn-danger">
                <i class="ace-icon fa fa-cogs"></i>
            </button>
        </div>
        <div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
            <span class="btn btn-success"></span>

            <span class="btn btn-info"></span>

            <span class="btn btn-warning"></span>

            <span class="btn btn-danger"></span>
        </div>
    </div><!-- /.sidebar-shortcuts -->

    <ul class="nav nav-list" style="top: 0px;">
        <li class="" ng-repeat="x in OnlineUsers track by $index">
            <a ng-click="privateChatBoxProperties.isboxopened || openInPrivate(x.UserName,x.Email,x.ConnectionId,x.FullName);">
                <i class="menu-icon fa fa-user"></i>
                @*<img class="media-object img-circle" style="max-height:40px;" src="~/Images/noimage.jpg">*@
                <span class="menu-text"> {{x.FullName}} 
                    <span class="badge badge-primary" ng-model="privateMessageCount" ng-if="privateMessageCount.userEmail==x.Email" ng-show="!privateMessageCount.isRead">{{privateMessageCount.messageCount}}</span>
                </span>
            </a>

            <b class="arrow"></b>
        </li>
        <li class="active">
            <a>
                <span class="menu-text"> Other Users </span>
            </a>
            <b class="arrow"></b>
        </li>
    @Html.Partial("_allRelatedUsers")
       
    </ul><!-- /.nav-list -->

    <div class="sidebar-toggle sidebar-collapse hide" id="sidebar-collapse ">
        <i id="sidebar-toggle-icon" class="ace-icon fa fa-angle-double-left ace-save-state" data-icon1="ace-icon fa fa-angle-double-left" data-icon2="ace-icon fa fa-angle-double-right"></i>
    </div>
</div>